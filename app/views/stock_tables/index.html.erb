<h1>Stock Tables</h1>

<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_tables.each do |stock_table| %>
      <tr>
        <td><%= stock_table.ticker %></td>
        <td><%= stock_table.user_id %></td>
        <td><%= link_to 'Show', stock_table %></td>
        <td><%= link_to 'Edit', edit_stock_table_path(stock_table) %></td>
        <td><%= link_to 'Destroy', stock_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock Table', new_stock_table_path %>
